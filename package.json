{
  "name": "react-redux-electron-starter-kit",
  "version": "1.0.0",
  "description": "An Electron starter with Dave Zuko's React Redux Starter Kit intergration",
  "main": "main.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "build": {
    "productName": "React Redux Electron Starter",
    "appId": "YourBundleId",
    "app-category-type": "public.app-category.business",
    "win": {
      "iconUrl": "",
      "setupIcon": ""
    },
    "osx": {
      "icon": "app_build/ico_installer_mac.icns"
    },
    "asar": true
  },
  "directories": {
    "output": "release",
    "buildResources": "app_build"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile:prod": "better-npm-run compile:prod",
    "compile:electron": "better-npm-run compile:electron",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "start-app": "better-npm-run start-app",
    "start-app-hot": "better-npm-run start-app-hot",
    "dev": "better-npm-run dev",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "flow:check": "babel-node bin/flow-check",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "dist:pre": "node bin/build-app-packagejson && install-app-deps && npm run compile:prod && npm run compile:electron",
    "package:linux": "better-npm-run package:pre && node bin/download-node --p linux --a x64 --d && build linux --arch x64",
    "package:mac": "better-npm-run package:pre && build --mac",
    "package:win32": "better-npm-run package:pre && build win32 --arch x64 --dist"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "betterScripts": {
    "compile:prod": {
      "command": "babel-node bin/compile && cp main.js app/",
        "env": {
        "NODE_ENV": "production"
      }
    },
    "compile:electron": {
      "command": "babel -d app/dist/electron electron",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "package:pre": {
      "command": "node bin/build-app-packagejson && install-app-deps && npm run compile:prod && npm run compile:electron",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start-app": {
      "command": "electron .",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start-app-hot": {
      "command": "HOT=1 NODE_ENV=development electron .",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donnycrash/react-redux-electron-starter.git"
  },
  "author": "Donovan Crewe <donnycrash@gmail.com>",
  "contributors": [
    {
      "name": "David Zukowski",
      "email": "david@zuko.me"
    }
  ],
  "bugs": {
    "url": "https://github.com/donnycrash/react-redux-electron-starter/issues"
  },
  "keywords": [
    "nodejitsu",
    "example",
    "browsenpm"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.2",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.0.8",
    "css-loader": "^0.23.0",
    "cssnano": "^3.3.2",
    "debug": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.8.4",
    "fs-extra": "^0.28.0",
    "history": "^2.0.0",
    "html-webpack-plugin": "^2.7.1",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "koa": "^2.0.0-alpha.3",
    "koa-connect-history-api-fallback": "^0.3.0",
    "koa-convert": "^1.2.0",
    "koa-proxy": "^0.5.0",
    "koa-static": "^2.0.0",
    "node-sass": "^3.3.3",
    "postcss-loader": "^0.8.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.0.0",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.1",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "yargs": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.0-beta.6",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-enzyme": "^0.4.0",
    "cheerio": "^0.20.0",
    "codecov": "^1.0.1",
    "electron-builder": "^5.11.2",
    "electron-prebuilt": "^1.2.6",
    "enzyme": "^2.0.0",
    "eslint": "^2.4.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^2.2.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-flow-vars": "^0.3.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^5.0.0",
    "eslint-plugin-standard": "^1.3.1",
    "flow-bin": "0.23.0",
    "flow-interfaces": "^0.6.0",
    "isparta-loader": "^2.0.0",
    "karma": "^0.13.21",
    "karma-coverage": "^0.5.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack-with-fast-source-maps": "^1.9.2",
    "mocha": "^2.2.5",
    "nodemon": "^1.8.1",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.3",
    "react-addons-test-utils": "^15.0.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.6.0",
    "webpack-target-electron-renderer": "^0.4.0"
  }
}
